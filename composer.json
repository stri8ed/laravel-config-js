{
    "name": "stri8ed/laravel-config-js",
    "description": "Laravel package for exposing configs in JS",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Stri8ed\\LaravelConfigJs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stri8ed\\LaravelConfigJs\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Levi Melamed",
            "email": "levi@levimelamed.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.74|^9.0|^10.0|^11.0",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0 || ^8.0 || ^9.0",
        "phpunit/phpunit": "^9.5.10|^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stri8ed\\LaravelConfigJs\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
